{
  "src": "./src/",
  "dest": "./dist/",
  "docs": {
    "src": "./docs/src/",
    "dest": "./docs/"
  },
  "build": "./build/",
  "styles": {
    "src": "./src/",
    "dest": "./dist/",
    "main": "locomotive-scroll",
    "docs": {
      "src": "./docs/src/styles/",
      "dest": "./docs/dist/styles/",
      "main": "main"
    }
  },
  "scripts": {
    "src": "./src/",
    "dest": "./dist/",
    "main": "locomotive-scroll",
    "docs": {
      "src": "./docs/src/scripts/",
      "dest": "./docs/dist/scripts/",
      "main": "main"
    }
  },
  "svgs": {
    "src": "./docs/src/images/sprite/",
    "dest": "./docs/dist/images/"
  },
  "views": {
    "src": "./docs/"
  },
  "modules": {
    "build": "gulp",
    "style": "sass",
    "script": "js",
    "view": false
  }
}
