ng: 1.5rem 0;\r\n  background-color: var(--light-dark-color);\r\n  transition: transform 0.5s ease, background 0.5s ease-in-out;\r\n}\r\n\r\nnav[data-direction=\"down\"] {\r\n  transform: translate3d(0, -100%, 0);\r\n}\r\n\r\n.nav .inner__nav {\r\n  width: 90%;\r\n  margin: 0 auto;\r\n}\r\n\r\n.nav .inner__nav .logo {\r\n  font-size: var(--h6-font);\r\n  font-weight: bold;\r\n  color: var(--theme-color);\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.nav .inner__nav .nav__links {\r\n  width: fit-content;\r\n}\r\n\r\n.nav .inner__nav .nav__links a {\r\n  padding: 0.5rem 1.2rem;\r\n  font-weight: 400;\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.nav .inner__nav .nav__links a:hover {\r\n  color: var(--theme-color);\r\n}\r\n\r\n.nav .inner__nav .nav__links a::after {\r\n  position: absolute;\r\n  z-index: 2;\r\n  width: 100%;\r\n  height: 2px;\r\n  transform: scaleX(0);\r\n  transform-origin: right;\r\n  background-color: var(--theme-color);\r\n  content: \"\";\r\n  bottom: 0;\r\n  left: 0;\r\n  transition: transform 0.5s cubic-bezier(0.65, 0.05, 0.36, 1);\r\n}\r\n\r\n.nav .inner__nav .nav__links a:hover::after {\r\n  transform-origin: left;\r\n  transform: scaleX(1);\r\n}\r\n\r\n/* footer */\r\n\r\nfooter {\r\n  width: 100%;\r\n  height: auto;\r\n  padding: 5rem 0;\r\n}\r\n\r\nfooter .container {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\nfooter .container a {\r\n  font-weight: 400;\r\n  cursor: pointer;\r\n  padding: 0.4rem 0.4rem;\r\n}\r\n\r\nfooter .container p {\r\n  font-size: 1rem;\r\n  letter-spacing: 1px;\r\n}\r\n\r\nfooter .container a:hover {\r\n  color: var(--theme-color);\r\n}\r\n\r\nfooter .container a::after {\r\n  position: absolute;\r\n  z-index: 2;\r\n  width: 100%;\r\n  height: 2px;\r\n  transform: scaleX(0);\r\n  transform-origin: right;\r\n  background-color: var(--theme-color);\r\n  content: \"\";\r\n  bottom: 0;\r\n  left: 0;\r\n  transition: transform 0.5s cubic-bezier(0.65, 0.05, 0.36, 1);\r\n}\r\n\r\nfooter .container a:hover::after {\r\n  transform-origin: left;\r\n  transform: scaleX(1);\r\n}\r\n\r\n/* next */\r\n\r\n/* next btn */\r\n\r\n.next__btn {\r\n  width: 100%;\r\n  min-height: 50vh;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  text-align: center;\r\n}\r\n\r\n.next__btn .container {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.next__btn .container a {\r\n  font-size: var(--h4-font);\r\n  font-family: var(--robo-font);\r\n  font-weight: 500;\r\n}\r\n\r\n.next__btn .container h4 {\r\n  color: var(--theme-color);\r\n  margin-top: 1rem;\r\n  font-weight: 400;\r\n  font-size: var(--a-font);\r\n}\r\n\r\n.next__btn .container a h1 {\r\n  position: relative;\r\n  width: fit-content;\r\n  padding: 0.4rem 1rem;\r\n}\r\n\r\n.next__btn .container a h1::after {\r\n  position: absolute;\r\n  z-index: 2;\r\n  width: 100%;\r\n  height: 2px;\r\n  transform: scaleX(0);\r\n  transform-origin: right;\r\n  background-color: var(--theme-color);\r\n  content: \"\";\r\n  bottom: 0;\r\n  left: 0;\r\n  transition: transform 0.5s cubic-bezier(0.65, 0.05, 0.36, 1);\r\n}\r\n\r\n.next__btn .container a h1:hover::after {\r\n  transform-origin: left;\r\n  transform: scaleX(1);\r\n}\r\n\r\n/* overlay */\r\n\r\n.overlay__main {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  background-color: var(--black-color);\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 1000;\r\n}\r\n\r\n/* hamb start */\r\n\r\n.hamb {\r\n  width: 4rem;\r\n  height: 4rem;\r\n  position: fixed;\r\n  top: 0.8rem;\r\n  right: 1rem;\r\n  z-index: 110;\r\n  cursor: pointer;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: space-around;\r\n  padding: 0.5rem 0;\r\n  display: none;\r\n}\r\n\r\n.hamb div {\r\n  transition: all 0.5s ease;\r\n  width: 3rem;\r\n  height: 2px;\r\n  border-radius: 20px;\r\n  background-color: var(--white-color);\r\n}\r\n\r\n.hamb.active div:nth-