  transition: background 0.5s ease;\r\n}\r\na {\r\n  color: var(--white-color);\r\n}\r\n\r\n/* custom style */\r\n\r\n.has-scroll-smooth body {\r\n  overflow: hidden;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n\r\n/* banner */\r\n\r\n.banner {\r\n  width: 100%;\r\n  height: 100%;\r\n  padding: 20vh 0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.banner .container {\r\n  width: 100%;\r\n  height: fit-content;\r\n}\r\n\r\n.banner .container .text__box {\r\n  width: fit-content;\r\n}\r\n\r\n.banner .container .text__box h1 {\r\n  font-size: var(--h3-font);\r\n  font-weight: bold;\r\n  font-family: var(--robo-font);\r\n  text-transform: capitalize;\r\n  letter-spacing: 3px;\r\n}\r\n\r\n.banner .container .text__box:nth-of-type(2) {\r\n  margin-top: 2rem;\r\n}\r\n\r\n.banner .container .text__box h4 {\r\n  color: var(--theme-color);\r\n  font-size: var(--a-font);\r\n  font-weight: 400;\r\n  letter-spacing: 1px;\r\n}\r\n\r\n/* portfolio */\r\n\r\n.portfolio {\r\n  width: 100%;\r\n  height: auto;\r\n  position: relative;\r\n  overflow: hidden;\r\n  padding: 5vh 0;\r\n}\r\n\r\n.portfolio .container {\r\n  width: 100%;\r\n  max-width: 1400px;\r\n  height: auto;\r\n}\r\n\r\n.portfolio .container .work {\r\n  width: 100%;\r\n  height: fit-content;\r\n  padding: 2rem 0;\r\n}\r\n\r\n.portfolio .container .work__rev {\r\n  flex-direction: row-reverse;\r\n}\r\n\r\n.portfolio .container .work__pic {\r\n  max-width: 60%;\r\n  width: 100%;\r\n  height: auto;\r\n  min-height: 80vh;\r\n  flex: 0 0 60%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.portfolio .container .work__content {\r\n  flex: 0 0 40%;\r\n  width: 100%;\r\n  height: auto;\r\n  padding-left: 4rem;\r\n  max-width: 40%;\r\n}\r\n\r\n.portfolio .container .work__pic .img__box {\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: hidden;\r\n  position: relative;\r\n  transition: all 0.5s linear;\r\n  will-change: transform;\r\n}\r\n\r\n.portfolio .container .work__pic .img__box img {\r\n  width: 100%;\r\n  height: 100%;\r\n  min-height: 35rem;\r\n  transform-origin: center;\r\n  transition: transform 1s ease;\r\n}\r\n\r\n.portfolio .container .work__pic .img__box img:hover {\r\n  transform: scale(1.1);\r\n}\r\n\r\n.portfolio .container .work__content h1 {\r\n  font-family: var(--robo-font);\r\n  font-size: var(--h5-font);\r\n  font-weight: 600;\r\n  letter-spacing: 2px;\r\n}\r\n\r\n.portfolio .container .work__content h4 {\r\n  font-size: var(--a-font);\r\n  color: var(--theme-color);\r\n  font-weight: 400;\r\n  /* margin-top: 0.5rem; */\r\n}\r\n\r\n/* webgl  container  */\r\n\r\n#canvas {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  z-index: 1;\r\n  width: 100%;\r\n  height: 100%;\r\n  pointer-events: none;\r\n}\r\n\r\n/* overlay */\r\n\r\n.overlay {\r\n  position: fixed;\r\n  z-index: 1000;\r\n  width: 100%;\r\n  height: 100%;\r\n  top: 0;\r\n  left: 0;\r\n  background-color: var(--black-color);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"801b7ebfca5e4aca9f960b5ed531bf30","cacheData":{"env":{}}}